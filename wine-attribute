    "# calculating the VIF for features selections"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 30,
   "metadata": {},
   "outputs": [],
   "source": [
    "import statsmodels.api as sm\n",
    "from statsmodels.stats.outliers_influence import variance_inflation_factor"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 31,
   "metadata": {},
   "outputs": [],
   "source": [
    "# For each X, calculate VIF and save in dataframe\n",
    "\n",
    "# red\n",
    "vif_red = pd.DataFrame()\n",
    "vif_red[\"VIF Factor\"] = [variance_inflation_factor(X_red.values, i) for i in range(X_red.shape[1])]\n",
    "vif_red[\"features\"] = X_red.columns"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 32,
   "metadata": {},
   "outputs": [],
   "source": [
    "# white\n",
